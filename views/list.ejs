<%- include("header") -%>
    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>
    
    <div class="box">
        <% for (let i=0; i<newListItems.length; i++){ %>
            <div class="item">
                <input type="checkbox">
                <p><%= newListItems[i] %></p>
            </div>    
        <% } %>
        <form class="item" action="/" method="post">
            <input name="addItem" type="text" placeholder="New Item">
            <button name="list" type="submit" value=<%= listTitle %>>+</button>
        </form> 

    </div>
<%- include("footer") -%>